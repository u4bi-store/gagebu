{
  "name": "gagebu",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack && tsc --project tsconfig-server.json",
    "server": "NODE_ENV=poduction node dist/bin/www",
    "dev": "concurrently 'nodemon' 'npm run dev:client'",
    "dev:server": "DEBUG=gagebu:* node --inspect=5858 -r ts-node/register ./src/bin/www.ts",
    "dev:client": "webpack-dev-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "assetPath": "assets",
  "outputPath": "dist",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./src/server"
    ],
    "exec": "npm run dev:server",
    "ext": "ts"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.0",
    "@types/moment": "^2.13.0",
